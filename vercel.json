{
  "version": 2,
  "builds": [
    {
      "src": "api/gerar.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/gerar",
      "dest": "/api/gerar.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
